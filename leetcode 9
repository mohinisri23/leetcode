//c solution for 9 Palindrome Number
bool isPalindrome(int x) {
    if(x<0){
        return false;
    }
    long int rev=0;
    long int temp=x;
    while(temp!=0){
        int n= temp%10;
        rev=rev*10+n;
        temp/=10;
    }
    if(rev==x){

        return true;
    }
    else{
        return false;
    }

    
}
