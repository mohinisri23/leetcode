//C++ solution for leetcode problem number 268 (Missing Number)
class Solution {
public:
    int missingNumber(vector<int>& nums) {
        sort(nums.begin(), nums.end());
        int n=nums.size();
        for(int i=0;i<n;i++) {
            if(nums[i]!=i){
                return i;
                break;
                
            }
        }
        return n;
        
    }
};
