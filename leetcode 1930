#python3 solution for leetcode problem number 1930 (Unique Length-3 Palindrome Subsequences)
class Solution:
    def countPalindromicSubsequence(self, s: str) -> int:
        character=set(s)
        result=0

        for c in character :
            i,j= s.index(c), s.rindex(c)
            middle=set()

            for k in range(i+1,j):
                middle.add(s[k])
            result+=len(middle)
        return result
            
        
